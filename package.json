{
  "name": "cashlypay",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./bin/www",
    "test": "NODE_ENV=development nodemon -e js,pug,css ./bin/www",
    "build": "npm run build:css",
    "build:css": "postcss public/stylesheets/main.css -o public/stylesheets/main.min.css",
    "lint": "eslint .",
    "seed": "NODE_ENV=sandbox node ./bin/script/seed-data.js generate",
    "seed:clear": "NODE_ENV=sandbox node ./bin/script/seed-data.js clear"
  },
  "dependencies": {
    "asynckit": "^0.4.0",
    "axios": "^1.12.2",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "commander": "^6.0.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "joi": "^17.11.0",
    "mime-db": "^1.54.0",
    "morgan": "^1.10.0",
    "pug": "^3.0.0",
    "serve-favicon": "^2.5.0",
    "square": "^25.0.0",
    "tslib": "^2.6.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
